{
    "manifest_version": 2,
    "name": "Bookmark Extension",
    "version": "1.0",
    "description": "A simple bookmark extension",
    "background": {
        "scripts": ["script.js"],
        "persistent": true
      },
    "icons": {
      "48": "logo.jpeg"
    },
    "browser_action": {
      "default_popup": "index.html"
    },
    "permissions": [
      "tabs",
      "bookmarks"
    ]
  }
  